<div class="content-wrapper">
<!-- Content Header (Page header) -->
<section class="content-header">
  <h1>
      Autocomplete Demo
    <small>using jquery-ui</small>

  </h1>
  <ol class="breadcrumb">
    <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
    <li><a href="#">Autocomplete</a></li>
  </ol>
</section>

<!-- Main content -->
<section class="content">
  <div class="row">
    <!-- left column -->
    <div class="col-md-6">
      <!-- general form elements -->
      <div class="box box-primary">
        <div class="box-header with-border">
          <h3 class="box-title">Quick Example</h3>
        </div><!-- /.box-header -->
        <!-- form start -->
        <form role="form">
          <div class="box-body">
            <div class="form-group">
              <label>Rut</label>
              <%= text_field_tag( 'rut', nil, :id => 'rut', data: {autocomplete_source: users_path } )%>
              <p>Autocompletar con datos via json usando jquery. Falto realizar que se actualicen los dem√°s campos</p>
            </div>

            <div class="form-group">
              <label>Lastname</label>
              <input type="text" class="form-control" id="lastname" placeholder="Enter a Lastname">
            </div>

            <div class="form-group">
              <label >email</label>
              <input type="text" class="form-control" id="email" placeholder="Enter an Email">
            </div>
          </div><!-- /.box-body -->

          <div class="box-footer">

          </div>
        </form>
      </div><!-- /.box -->
    </div><!--/.col (right) -->
  </div>   <!-- /.row -->
</section><!-- /.content -->
</div><!-- /.content-wrapper -->


